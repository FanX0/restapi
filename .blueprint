    - database/factories/VariationFactory.php
    - database/factories/CartFactory.php
    - database/factories/TransactionFactory.php
    - database/factories/ShippingAddressFactory.php
    - database/migrations/2024_03_09_095245_create_users_table.php
    - database/migrations/2024_03_09_095246_create_categories_table.php
    - database/migrations/2024_03_09_095247_create_products_table.php
    - database/migrations/2024_03_09_095248_create_variations_table.php
    - database/migrations/2024_03_09_095249_create_carts_table.php
